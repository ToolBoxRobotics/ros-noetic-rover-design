
<launch>
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_odom" output="screen">
    <param name="use_sim_time" value="false"/>
    <rosparam command="load" file="$(find rover_bringup)/config/ekf_odom.yaml"/>
  </node>

  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_map" output="screen">
    <param name="use_sim_time" value="false"/>
    <rosparam command="load" file="$(find rover_bringup)/config/ekf_map.yaml"/>
  </node>
</launch>
